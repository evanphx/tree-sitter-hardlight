{
  "name": "tree-sitter-hardlight",
  "version": "0.0.1",
  "description": "hardlight grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  },
  "scripts": {
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.hardlight",
      "file-types": [
        "hl"
      ],
      "injection-regex": "hardlight"
    }
  ]
}
